<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<GridPane fx:controller="FTPClient.Controller" xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <columnConstraints>
        <ColumnConstraints percentWidth="25.0"/>
        <ColumnConstraints percentWidth="25.0"/>
        <ColumnConstraints percentWidth="25.0"/>
        <ColumnConstraints percentWidth="25.0"/>
    </columnConstraints>
    <TextField GridPane.rowIndex="0" GridPane.columnIndex="0" fx:id="fromField"/>
    <TextField GridPane.rowIndex="0" GridPane.columnIndex="2" fx:id="toField"/>
    <Button GridPane.rowIndex="0" GridPane.columnIndex="3" fx:id="getFileButton" text="Get File" onAction="#getFile"/>
</GridPane>